fizzBuzz(n) {
    index = 1;
    while (index <= n) {
        if (index % 3 == 0 & index % 5 == 0) {
            print("FizzBuzz\n");
        } elif (index % 3 == 0) {
            print("Fizz\n");
        } elif (index % 5 == 0) {
            print("Buzz\n");
        } else {
            print(index, "\n");
        }
        index += 1;
    }
}

main() {
    fizzBuzz(100);
}
